{
  "manifest_version": 2,

  "name": "Fullscreen Schema Builder",
  "version": "0.1.0",
  "description": "Description here",

  "icons": {
    "16" : "img/icons/16.png",
    "48" : "img/icons/48.png",
    "128" : "img/icons/128.png"
  },

  "browser_action": {
  "default_icon": "img/tquila_lozenge.png"
